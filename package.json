    "@angular/router": "^17.3.0",
    "bootstrap-slider": "^11.0.2",
    "core-js": "^3.37.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.6",
    "@angular/cli": "^17.0.0",
