{
    "compileOnSave": false,
    "compilerOptions": {
      "baseUrl": "./",
      "downlevelIteration": true,
      "module": "esnext",
      "outDir": "./dist/out-tsc",
      "sourceMap": true,
      "declaration": false,
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "target": "es2022",
      "typeRoots": [
        "node_modules/@types"
      ],
      "lib": [
        "es2022",
        "dom"
      ],
      "paths": {
        "ngx-bootstrap-slider": [
          "projects/ngx-bootstrap-slider/src/public_api"
        ],
        "ngx-bootstrap-slider/*": [
          "projects/ngx-bootstrap-slider/src/public_api"
        ]
      }
    }
  }